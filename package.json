    "set-node-v": "pnpm env use --global 20"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.2",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "dotenv-cli": "^7.4.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "turbo": "^1.13.3"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "pnpm run lint",
      "pnpm run format"
    ]
  }
