{
  "name": "quackdb",
  "version": "0.0.2",
  "private": true,
  "type": "module",
  "repository": {
    "url": "https://github.com/mattf96s/quackdb"
  },
  "author": {
    "name": "Matthew Fainman"
  },
  "description": "QuackDB - An online DuckDB SQL playground and editor",
  "scripts": {
    "build": "turbo build",
